{
  "name": "obsideck",
  "version": "1.1.4",
  "description": "A Stream Deck plugin that runs commands in Obsidian",
  "type": "module",
  "scripts": {
    "check": "biome check --write src",
    "format": "biome format --write src",
    "lint": "biome lint src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "validate": "pnpm exec streamdeck validate src/dev.jduabe.obsideck.sdPlugin"
  },
  "keywords": [],
  "author": "Jordan Duabe <me@jduabe.dev>",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@elgato/cli": "^1.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7"
  },
  "lint-staged": {
    "*.{js,html,css}": [
      "biome check --write --files-ignore-unknown=true"
    ]
  }
}
